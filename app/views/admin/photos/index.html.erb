<div class="container">
	<h2 class="text-center pb-3">フォト一覧</h2>
	<div class="row">

		<table class="table table-hover table-bordered">
			<thead>
				<tr>
					<th>フォトID</th>
					<th>フォト名</th>
				</tr>
			</thead>
			<tbody>
				<% @photos.each do |photo| %>
					<tr>
						<td class="text-center"><%= photo.id %></td>
						<td><%= link_to photo.name, admin_photo_path(photo) %></td>
					</tr>
				<% end %>
			</tbody>
		</table>

		<%= paginate @photos %>

  </div>
</div>