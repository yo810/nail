<div class="container title">
	<h2 class="text-center pb-3">Photo</h2>
	<div class="row justify-content-center text-center">

		<table class="table table-hover table-bordered w-75">
			<thead>
				<tr>
					<th>Photo ID</th>
					<th>Photo Âêç</th>
				</tr>
			</thead>
			<tbody>
				<% @photos.each do |photo| %>
					<tr>
						<td><%= photo.id %></td>
						<td><%= link_to photo.name, admin_photo_path(photo) %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
  </div>
		<%= paginate @photos %>
</div>